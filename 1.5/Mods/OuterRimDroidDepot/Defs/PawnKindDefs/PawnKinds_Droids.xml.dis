		<defName>OuterRim_ImperialKXSecurityDroid</defName>
		<label>KX Security Droid</label>
		<labelPlural>Security Droids</labelPlural>
		<race>OuterRim_KXSecurityDroid</race>
		<isFighter>true</isFighter>
		<backstoryFiltersOverride>
			<li>
				<categories>
					<li>KXSecurityDroidBackstory</li>
				</categories>
			</li>
