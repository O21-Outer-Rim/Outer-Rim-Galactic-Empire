<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	<ThingDef ParentName="OuterRim_MediumHelmetFabricatedBase">
		<defName>OuterRim_ISBAgentHelmet</defName>
		<label>isb agent helmet</label>
		<description>An armoured helmet used by imperial security bureau agents.</description>
		<graphicData>
			<texPath>OuterRim/Apparel/ImperialUniform/ISBAgent/Head</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<apparel>
			<tags Inherit="False">
				<li>ImperialApparel</li>
				<li>ImperialAgent</li>
			</tags>
			<wornGraphicPath>OuterRim/Apparel/ImperialUniform/ISBAgent/Head</wornGraphicPath>
			<useWornGraphicMask>true</useWornGraphicMask>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>(255, 255, 255)</only>
				</li>
			</options>
		</colorGenerator>
		<thingSetMakerTags>
			<li>OuterRim_ApparelGalacticEmpire</li>
		</thingSetMakerTags>
	</ThingDef>
	
</Defs>