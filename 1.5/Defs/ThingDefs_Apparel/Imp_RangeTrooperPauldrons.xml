			<wornGraphicPath>OuterRim/Apparel/Stormtrooper/RangePauldrons/Apparel</wornGraphicPath>
			<useWornGraphicMask>false</useWornGraphicMask>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>(255, 255, 255)</only>
				</li>
			</options>
		</colorGenerator>
		<thingSetMakerTags>
			<li>OuterRim_ApparelGalacticEmpire</li>
		</thingSetMakerTags>
	</ThingDef>

</Defs>