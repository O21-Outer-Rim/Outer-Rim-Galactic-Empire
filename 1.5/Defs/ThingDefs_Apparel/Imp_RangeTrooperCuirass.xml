<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	<ThingDef ParentName="OuterRim_HeavyArmorFabricatedBase">
		<defName>OuterRim_RangeTrooperCuirass</defName>
		<label>range trooper cuirass</label>
		<description>An armoured cuirass used by the Galactic Empire in the elite Range Corps.</description>
		<graphicData>
			<texPath>OuterRim/Apparel/Stormtrooper/SnowCuirass/Apparel</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<color>(255, 255, 255)</color>
		</graphicData>
		<statBases>
			<Insulation_Cold>45</Insulation_Cold>
		</statBases>
		<equippedStatOffsets>
			<ToxicEnvironmentResistance>0.4</ToxicEnvironmentResistance>
		</equippedStatOffsets>
		<apparel>
			<tags Inherit="False">
				<li>ImperialApparel</li>
				<li>ImperialRangeTrooper</li>
			</tags>
			<wornGraphicPath>OuterRim/Apparel/Stormtrooper/SnowCuirass/Apparel</wornGraphicPath>
			<useWornGraphicMask>true</useWornGraphicMask>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options" Inherit="False">
			<options>
				<li>
					<weight>10</weight>
					<only>(255, 255, 255)</only>
				</li>
			</options>
		</colorGenerator>
		<thingSetMakerTags>
			<li>OuterRim_ApparelGalacticEmpire</li>
		</thingSetMakerTags>
	</ThingDef>
	
</Defs>