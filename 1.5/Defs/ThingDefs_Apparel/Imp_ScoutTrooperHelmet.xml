<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef ParentName="OuterRim_LightHelmetFabricatedBase">
		<defName>OuterRim_ScoutTrooperHelmet</defName>
		<label>scout trooper helmet</label>
		<description>An armoured helmet used by the Galactic Empire in the Stormtrooper Corps.</description>
		<graphicData>
			<texPath>OuterRim/Apparel/Stormtrooper/ScoutHelmet/Head</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<color>(255, 255, 255)</color>
		</graphicData>
		<equippedStatOffsets>
			<ToxicEnvironmentResistance>0.3</ToxicEnvironmentResistance>
		</equippedStatOffsets>
		<apparel>
			<immuneToToxGasExposure>true</immuneToToxGasExposure>
			<tags Inherit="False">
				<li>ImperialApparel</li>
				<li>ImperialScout</li>
			</tags>
			<wornGraphicPath>OuterRim/Apparel/Stormtrooper/ScoutHelmet/Head</wornGraphicPath>
			<useWornGraphicMask>false</useWornGraphicMask>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>(255, 255, 255)</only>
				</li>
			</options>
		</colorGenerator>
		<thingSetMakerTags>
			<li>OuterRim_ApparelGalacticEmpire</li>
		</thingSetMakerTags>
	</ThingDef>

</Defs>