<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef ParentName="StormtrooperArmorPauldronBase">
		<defName>OuterRim_SnowtrooperPauldrons</defName>
		<label>snowtrooper pauldrons (pride)</label>
		<description>Armoured pauldrons used by the Galactic Empire in the Stormtrooper Corps. This variant is used for cold weather assaults, providing increased protection against the cold.</description>
		<graphicData>
			<texPath>OuterRim/Apparel/Stormtrooper/SnowPauldrons/Apparel</texPath>
			<shaderType>Cutout</shaderType>
			<color>(255, 255, 255)</color>
		</graphicData>
		<statBases>
			<Insulation_Cold>45</Insulation_Cold>
		</statBases>
		<apparel>
			<tags Inherit="False">
				<li>ImperialApparel</li>
				<li>ImperialSnowtrooper</li>
			</tags>
			<wornGraphicPath>OuterRim/Apparel/Stormtrooper/SnowPauldrons/Apparel</wornGraphicPath>
			<useWornGraphicMask>false</useWornGraphicMask>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options" Inherit="False">
			<options>
				<li>
					<weight>10</weight>
					<only>(255, 255, 255)</only>
				</li>
			</options>
		</colorGenerator>
	</ThingDef>

</Defs>